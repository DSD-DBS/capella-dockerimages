set -eo pipefail

matches=(t4c/updateSite/$CAPELLA_VERSION/com.thalesgroup.mde.melody.team.license.update-*.zip)
if [ ${#matches[@]} != 1 ]; then
    echo "Couldn't find com.thalesgroup.mde.melody.team.license.update-*.zip in /app/t4c/updateSite/$CAPELLA_VERSION."
    exit 1
fi

echo $matches
